{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "wrap-iife": 0,
    "arrow-parens": 0,
    "quotes": 0,
    "comma-dangle": 0,
    "no-nested-ternary": 0,
    "no-shadow":0,
    "operator-linebreak": 0
  }
}
